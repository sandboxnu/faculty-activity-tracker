{
  "name": "faculty-activity-tracker",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "dev:db:up": "docker-compose up -d",
    "dev:db:down": "docker-compose down",
    "migrate": "yarn prisma migrate dev --name init",
    "migrate:dev": "DATABASE_URL='postgresql://sandbox:chongus@localhost:5432/fat?schema=public' yarn prisma migrate dev",
    "generate": "yarn prisma generate",
    "seed": "yarn prisma db seed",
    "format:check": "prettier --check --ignore-path .gitignore .",
    "format": "prettier --write --ignore-path .gitignore .",
    "prisma:studio:dev": "DATABASE_URL='postgresql://sandbox:chongus@localhost:5432/fat?schema=public' npx prisma studio",
    "prepare": "husky install"
  },
  "lint-staged": {
    "**/*.(ts|tsx|js)": [
      "prettier --write --ignore-path .gitignore ."
    ],
    "**/*.(md|json)": "prettier --write --ignore-path .gitignore ."
  },
  "dependencies": {
    "@prisma/client": "4.9.0",
    "@reduxjs/toolkit": "^1.9.2",
    "@svgr/webpack": "^6.5.1",
    "@types/node": "^18.11.19",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "clsx": "^2.0.0",
    "eslint": "8.33.0",
    "eslint-config-next": "13.1.6",
    "moment": "^2.29.4",
    "next": "^13.5.6",
    "next-auth": "^4.19.0",
    "prisma": "4.9.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "^8.0.5",
    "react-responsive-carousel": "^3.2.23",
    "typescript": "^4.9.5"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.13",
    "husky": "^8.0.0",
    "postcss": "^8.4.21",
    "prettier": "2.8.4",
    "sass": "^1.57.1",
    "tailwindcss": "^3.2.4",
    "ts-node": "^10.9.1"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts",
    "schema": "prisma/schema.prisma"
  }
}
